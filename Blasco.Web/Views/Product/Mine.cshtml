@model List<ProductAllViewModel>
@{
    ViewData["Title"] = "My Products";
}

<h2 calss="text-center"> @ViewData["Title"]</h2>
<hr />

@if (!Model.Any())
{
    <h2 calss="text-center">"You do not have any Products yet"</h2>
}
else
{
    <div class=" row">
        @foreach (ProductAllViewModel product in Model)
        {
            <partial name="_ProductPartial" model="@product" />
        }
    </div>
}